    "import torch\n",
    "# 示例 2: 从向量创建对角矩阵 (返回2D tensor)\n",
    "vector = torch.tensor([0, 20, 30])\n",
    "diagonal_matrix = torch.diag(vector)\n",
    "print(\"\\n原向量:\\n\", vector)\n",
    "print(\"从向量创建的对角矩阵:\\n\", diagonal_matrix)\n",
    "# 输出:\n",
